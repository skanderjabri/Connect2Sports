{
  "expo": {
    "name": "Connect2sport",
    "slug": "connect2sport",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splashscren.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splashscren.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splashscren.png",
        "backgroundColor": "#ffffff"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splashscren.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.iskanderjabri.connect2sport"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 500,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Autorisez à accéder à votre position en permanence et lorsque l'application est en cours d'utilisation."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
